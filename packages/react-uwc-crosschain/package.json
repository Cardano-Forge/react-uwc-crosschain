{
  "name": "@ada-anvil/react-uwc-crosschain",
  "version": "1.0.5",
  "license": "CC BY-NC 4.0",
  "keywords": [
    "react",
    "uwc",
    "Universal wallet connector",
    "Anvil",
    "Ada anvil",
    "crosschain",
    "solana",
    "ethereum",
    "matic"
  ],
  "exports": {
    "./eth": {
      "types": "./dist/eth/index.ts",
      "import": "./dist/eth/index.js",
      "default": "./src/eth/index.ts"
    },
    "./sol": {
      "types": "./dist/sol/index.ts",
      "import": "./dist/sol/index.js",
      "default": "./src/sol/index.ts"
    },
    "./matic": {
      "types": "./dist/matic/index.ts",
      "import": "./dist/matic/index.js",
      "default": "./src/matic/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run ./watch.js --watch",
    "package:publish": "npm run build && npm publish --access public",
    "package:build": "bun run ./build.js && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "ethers": "^6.9.1",
    "jotai": "^2.6.0"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@turbo/gen": "^1.10.12",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "bun-types": "^1.0.20",
    "eslint": "^8.56.0",
    "typescript": "^5.2.2",
    "prettier": "^3.1.1"
  },
  "dependencies": {
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "ethers": "^6.9.1",
    "jotai": "^2.6.0",
    "lodash.get": "^4.4.2",
    "react": "^18.2.0"
  }
}
